{
  "name": "ExternalResourcesAssistant",
  "version": "0.8",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "\u5916\u90e8\u8d44\u6e90\u52a9\u624b",
    "default_popup": "popup.html"
  },
   "default_locale": "en",  
  "options_page":"options.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ]
}